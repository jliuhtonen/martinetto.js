{
  "name": "martinetto",
  "version": "1.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/jliuhtonen/martinetto.js.git"
  },
  "description": "A modular, functional JS route parser micro library",
  "main": "dist/main.js",
  "scripts": {
    "compile": "tsc",
    "postinstall": "typings install",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha --compilers js:babel-register",
    "bundle": "tsc && rollup --config"
  },
  "keywords": [
    "route",
    "parser",
    "micro-library"
  ],
  "author": "Janne Liuhtonen",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.2.4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "6.2.4",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "rollup": "^0.35.10",
    "rollup-plugin-cleanup": "^0.1.4",
    "rollup-plugin-commonjs": "^4.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "typescript": "^1.8.10",
    "typings": "^1.3.3"
  },
  "dependencies": {
    "object-assign": "^4.1.0",
    "parsimmon": "0.9.2"
  }
}
